<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Get Api suites" parallel="tests" thread-count="2">
    <listeners>  
	   <listener class-name="listeners.MethodIntercepter"/>  
	</listeners> 
	<test  parallel="methods" name="login_suite">
		<classes>
			<class name="testcases.LoginTest" />
		</classes>
	</test>
	<test parallel="methods" name="signup_suite">
		<classes>
			<class name="testcases.SignUpTest" />
		</classes>
	</test>
</suite>